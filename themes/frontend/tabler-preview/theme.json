{
  "name": "Tabler Preview",
  "slug": "tabler-preview",
  "version": "1.0.0-beta19",
  "type": "frontend",
  "author": "Tabler",
  "license": "MIT",
  "description": "Clean and elegant frontend theme based on Tabler",
  "homepage": "https://tabler.io",
